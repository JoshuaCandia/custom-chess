{
  "name": "custom-chess",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n server,client -c magenta,cyan \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm run dev --prefix client",
    "seed:puzzles": "npm run seed:puzzles --prefix server"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
